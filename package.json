{
  "name": "demoproject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "reset": "npx react-native start --reset-cache",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html",
    "pod": "npx pod-install ios",
    "pod:update": "cd ios && pod install --repo-update && cd ..",
    "android:clean": "cd android && ./gradlew clean",
    "ios:clean": "cd ios && pod deintegrate && rm -rf Podfile.lock",
    "clean:node_modules": "rm -rf node_modules && rm -rf yarn.lock",
    "link:assets": "npx react-native-asset",
    "android:staging": "cd android && ./gradlew clean && react-native run-android --mode=stagingdebug",
    "android:staging-release": "cd android && ./gradlew clean && react-native run-android --mode=stagingrelease",
    "android:dev": " cd android && ./gradlew clean && react-native run-android --mode=developmentdebug",
    "android:dev-release": "cd android && ./gradlew clean && react-native run-android --mode=developmentrelease",
    "android:prod": "cd android && ./gradlew clean && react-native run-android --mode=productiondebug",
    "android:prod-release": "cd android && ./gradlew clean && react-native run-android --mode=productionrelease",
    "ios:dev": "ENVFILE=.env.dev react-native run-ios --scheme 'DemoProjectDev' --simulator='iPhone 15'"
  },
  "dependencies": {
    "fbemitter": "^3.0.0",
    "i18next": "^23.11.2",
    "lodash.memoize": "^4.1.2",
    "react": "18.2.0",
    "react-i18next": "^14.1.0",
    "react-native": "0.73.6",
    "react-native-config": "^1.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native/babel-preset": "^0.74.81",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.3",
    "@react-native/typescript-config": "0.73.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-root-import": "^6.6.0",
    "eslint": "^8.19.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
